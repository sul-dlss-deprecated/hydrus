<workflow-def id="hydrusAssemblyWF" repository="dor">
  <process lifecycle="registered" name="start-deposit" sequence="1" status="completed">
    <label>Initiate assembly of the hydrus object</label>
  </process>
  <process name="submit" sequence="2">
    <prereq>start-deposit</prereq>
    <label>Start hydrus deposit</label>
  </process>
  <process name="approve" sequence="3">
    <prereq>submit</prereq>
    <label>Hydrus object approval</label>
  </process>
  <process name="start-assembly" sequence="4">
    <prereq>submit</prereq>
    <label>Start assemblyWF</label>
  </process>
</workflow-def>
